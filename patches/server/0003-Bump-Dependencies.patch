         exclude(group="org.yaml", module="snakeyaml")
     }
     // Pufferfish end
     implementation("com.github.technove:Flare:34637f3f87") // Pufferfish - flare
 
-    testImplementation("io.github.classgraph:classgraph:4.8.47") // Paper - mob goal test
